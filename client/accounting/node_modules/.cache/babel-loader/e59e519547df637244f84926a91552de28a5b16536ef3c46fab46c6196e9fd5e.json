   <ListItem>\r\n                    <ListItemText primary=\"Управление допуском сотрудников к опасным видам работ.\" />\r\n                </ListItem>\r\n                <ListItem>\r\n                    <ListItemText primary=\"Отслеживание прохождения обучения по безопасности и получения необходимых сертификатов.\" />\r\n                </ListItem>\r\n                <ListItem>\r\n                    <ListItemText primary=\"Генерация отчетов о состоянии допуска сотрудников к работам.\" />\r\n                </ListItem>\r\n                <ListItem>\r\n                    <ListItemText primary=\"Интеграция с другими системами учета и безопасности.\" />\r\n                </ListItem>\r\n            </List>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default HomePage;\r\n"],"mappings":";AAAA,OAAOA,KAAK,MAAM,OAAO;AACzB,SAASC,UAAU,EAAEC,IAAI,EAAEC,IAAI,EAAEC,QAAQ,EAAEC,YAAY,QAAQ,eAAe;AAAC,SAAAC,MAAA,IAAAC,OAAA;AAE/E,MAAMC,QAAQ,GAAGA,CAAA,KAAM;EACnB,oBACID,OAAA;IAAAE,QAAA,gBACIF,OAAA,CAACN,UAAU;MAACS,OAAO,EAAC,IAAI;MAACC,YAAY;MAAAF,QAAA,EAAC;IAEtC;MAAAG,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OAAY,CAAC,eACbR,OAAA,CAACN,UAAU;MAACS,OAAO,EAAC,OAAO;MAACM,KAAK,EAAE;QAAEC,YAAY,EAAE;MAAO,CAAE;MAAAR,QAAA,EAAC;IAK7D;MAAAG,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OAAY,CAAC,eACbR,OAAA,CAACN,UAAU;MAACS,OAAO,EAAC,IAAI;MAACC,YAAY;MAAAF,QAAA,EAAC;IAEtC;MAAAG,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OAAY,CAAC,eACbR,OAAA,CAACJ,IAAI;MAAAM,QAAA,gBACDF,OAAA,CAACH,QAAQ;QAAAK,QAAA,eACLF,OAAA,CAACF,YAAY;UAACa,OAAO,EAAC;QAAwD;UAAAN,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAE;MAAC;QAAAH,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAC3E,CAAC,eACXR,OAAA,CAACH,QAAQ;QAAAK,QAAA,eACLF,OAAA,CAACF,YAAY;UAACa,OAAO,EAAC;QAAwD;UAAAN,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAE;MAAC;QAAAH,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAC3E,CAAC,eACXR,OAAA,CAACH,QAAQ;QAAAK,QAAA,eACLF,OAAA,CAACF,YAAY;UAACa,OAAO,EAAC;QAAyF;UAAAN,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAE;MAAC;QAAAH,QAAA,EAA